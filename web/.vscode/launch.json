  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Frontend",
      "request": "launch",
      "type": "msedge",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/web",
      "cwd": "${workspaceFolder}/web",
      "sourceMapPathOverrides": {
        "localhost:*/src/*": "${webRoot}/src/*",
        "127.0.0.1:*/src/*": "${webRoot}/src/*"
      },
      "preLaunchTask": "pnpm: dev @backround"
    }
  ]
}
